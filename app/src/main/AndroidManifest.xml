<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.notebin">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_logo_round"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_logo_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".authentication.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".authentication.RegisterActivity" />
        <activity android:name=".attendance.AddAttendance" />
        <activity android:name=".attendance.Attendances" />
        <activity android:name=".expenses.AddExpense" />
        <activity android:name=".MainPage" />
        <activity android:name=".competition.Alerts" />
        <activity android:name=".notes.notes" />
        <activity android:name=".MainActivity"/>

        <receiver android:name=".NotifierAlarm" />
    </application>

</manifest>